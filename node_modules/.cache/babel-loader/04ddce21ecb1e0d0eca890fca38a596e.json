\n}\r\n\r\n\r\nconst mutations={\r\n  ADD_ITEM:(state,payload)=>{\r\n    state.buycar=payload\r\n  },\r\n  CHANGE_ITEM:(state,payload)=>{\r\n    state.buycar=payload\r\n  },\r\n  REMOVE_ITEM:(state,payload)=>{\r\n    state.buycar=payload\r\n  },\r\n  CLEAR_BUYCAR:(state,payload)=>{\r\n    state.buycar=payload\r\n  }\r\n}\r\n\r\n\r\nexport default{\n  namespaced:true,//命名空间的模块。模块内部直接调用，外部需要命名空间(见组件)\n  state,actions,mutations\n}"]}]}